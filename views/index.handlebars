<h1>{{title}}</h1>
<h4>Gaspardo Enterprises LLC</h4>
<ul class="list-group">
    {{#each match}}
    <li class="list-group-item">{{this.match}}: {{this.blue_1}}</li>
    {{/each}}
</ul>

<select id="selectmatch" onchange="selectmatch()">
    {{#each match}}
    <option value="{{this.match}}: {{this.blue_1}}">{{this.match}}: {{this.blue_1}}</option>
    {{/each}}
</select>